<h1>Listing sessions</h1>
 <div class="field">
    user id= <br />
    <%= session[:current_user_id] %>
  </div>

<table>
  <tr>
    <th>State</th>
    <th>Current question</th>
    <th>Final response</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sessions.each do |session| %>
  <tr>
    <td><%= session.state %></td>
    <td><%= session.current_question %></td>
    <td><%= session.final_response %></td>
    <td><%= link_to 'Show', session %></td>
    <td><%= link_to 'Edit', edit_session_path(session) %></td>
    <td><%= link_to 'Destroy', session, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

